{
  "compilerOptions": {
    "outDir": "ngcout",
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es2015", "dom"],
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    //"noUnusedLocals": true,
    "types": [
      "gapi",
      "gapi.client"
    ]
  },
  "files": [
    "src/main.ts"
  ]
}